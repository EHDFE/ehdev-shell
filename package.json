{
  "name": "ehdev-shell",
  "version": "0.0.5",
  "description": "ehdev's gui shell",
  "main": "main.js",
  "dependencies": {
    "boom": "^6.0.0",
    "electron-context-menu": "^0.9.1",
    "electron-is-dev": "^0.3.0",
    "find-free-port": "^1.0.2",
    "koa": "^2.3.0",
    "koa-better-http-proxy": "^0.2.4",
    "koa-body": "^2.5.0",
    "koa-router": "^7.2.1",
    "koa-send": "^4.1.1",
    "koa-views": "^6.1.0",
    "nedb": "^1.8.0",
    "nunjucks": "^3.0.1"
  },
  "devDependencies": {
    "antd": "^2.13.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "devtron": "^1.4.0",
    "electron": "^1.7.8",
    "electron-builder": "^19.35.1",
    "electron-react-devtools": "^0.4.0",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "koa-webpack-middleware": "^1.0.7",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "moment": "^2.19.0",
    "pre-commit": "^1.2.2",
    "pretty-file-icons": "^2.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dispatch": "^2.2.1",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "reselect": "^3.0.1",
    "shebang-loader": "0.0.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.6.0"
  },
  "pre-commit": ["precommit-msg", "lint"],
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "debug": "cross-env NODE_ENV=development electron --inspect=5858 .",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint --cache ./",
    "lint-fix": "eslint --cache ./ --fix",
    "dll-dev": "webpack --env.dev --config ./config/webpack.config.dll.js --progress --verbose",
    "dll-prod": "webpack --env.prod --config ./config/webpack.config.dll.js --progress",
    "build-prod": "webpack --env.prod --config ./config/webpack.config.js --progress",
    "pack": "electron-builder --dir",
    "dist-all": "electron-builder -mw",
    "dist-mac": "electron-builder -m",
    "dist-win": "electron-builder -w"
  },
  "build": {
    "productName": "EHD Shell",
    "appId": "com.ehdev.shell",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!.vscode",
      "!app",
      "!config",
      "!eslintrc.json",
      "!**/*.md"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "portable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EHDFE/ehdev-shell.git"
  },
  "author": "ryan.bian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EHDFE/ehdev-shell/issues"
  },
  "homepage": "https://github.com/EHDFE/ehdev-shell#readme"
}
