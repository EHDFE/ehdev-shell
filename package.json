{
  "name": "ehdev-shell",
  "version": "0.0.6",
  "description": "ehdev's gui shell",
  "main": "main.js",
  "dependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "boom": "^7.1.1",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "electron-context-menu": "^0.9.1",
    "electron-is-dev": "^0.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "find-free-port": "^1.0.2",
    "fix-path": "^2.1.0",
    "html-webpack-plugin": "^2.30.1",
    "koa": "^2.4.1",
    "koa-better-http-proxy": "^0.2.4",
    "koa-body": "^2.5.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.3.0",
    "koa-send": "^4.1.1",
    "koa-views": "^6.1.2",
    "nedb": "^1.8.0",
    "nunjucks": "^3.0.1",
    "raven": "^2.2.1",
    "react-syntax-highlighter": "^6.0.3",
    "stats-webpack-plugin": "^0.6.1",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "devDependencies": {
    "antd": "^3.0.0-beta.7",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "devtron": "^1.4.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.45.5",
    "electron-react-devtools": "^0.5.0",
    "eslint": "^4.10.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "koa-webpack-middleware": "^1.0.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "pre-commit": "^1.2.2",
    "pretty-file-icons": "^2.2.1",
    "prop-types": "^15.6.0",
    "raven-for-redux": "^1.1.1",
    "react": "^16.1.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dispatch": "^2.2.1",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "remarkable": "^1.7.1",
    "reselect": "^3.0.1",
    "semver": "^5.4.1",
    "shebang-loader": "0.0.1",
    "style-loader": "^0.19.0",
    "xterm": "^2.9.2"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "debug": "cross-env NODE_ENV=development electron --inspect=5858 .",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint --cache ./",
    "lint-fix": "eslint --cache ./ --fix",
    "dll-dev": "webpack --env.dev --config ./config/webpack.config.dll.js --progress --verbose",
    "dll-prod": "webpack --env.prod --config ./config/webpack.config.dll.js --progress",
    "build-prod": "webpack --env.prod --config ./config/webpack.config.js --progress",
    "pack": "electron-builder --dir",
    "dist-all": "electron-builder -mw",
    "dist-mac": "electron-builder -m",
    "dist-win": "electron-builder -w"
  },
  "build": {
    "productName": "Jarvis",
    "appId": "com.ehdev.jarvis",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!.vscode",
      "!app",
      "!config",
      "!eslintrc.json",
      "!**/*.md",
      "!**/node_modules/*/{*.map,README.md~,LICENSE,.*.yml,LICENSE-MIT}"
    ],
    "asar": true,
    "asarUnpack": [
      "src/child_service/*",
      "node_modules/webpack/**/*",
      "node_modules/webpack-dev-server/**/*",
      "node_modules/chalk/**/*",
      "node_modules/extract-text-webpack-plugin/**/*",
      "node_modules/clean-webpack-plugin/**/*",
      "node_modules/add-asset-html-webpack-plugin/**/*",
      "node_modules/stats-webpack-plugin/**/*",
      "node_modules/html-webpack-plugin/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "portable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EHDFE/ehdev-shell.git"
  },
  "author": "ryan.bian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EHDFE/ehdev-shell/issues"
  },
  "homepage": "https://github.com/EHDFE/ehdev-shell#readme"
}
