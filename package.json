    "webpack-stylish": "^0.1.8"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "scripts": {
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "debug": "cross-env NODE_ENV=development electron --inspect=5858 ./app/",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint --cache ./app",
    "lint-fix": "eslint --cache ./app --fix",
    "pack": "electron-builder --dir",
    "dist-all": "electron-builder -mwl",
    "dist-mac": "electron-builder -m",
    "dist-win": "electron-builder -w",
    "dist-linux": "electron-builder -l",
    "postinstall": "electron-builder install-app-deps",
    "build": "concurrently \"npm run build-main \" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development webpack --mode development --config ./config/webpack.config.dll.js --progress",
    "build-main": "cross-env NODE_ENV=production webpack --mode production --config ./config/webpack.config.main.prod.js --progress",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js --progress",
    "build-renderer-analyzer": "cross-env OPEN_ANALYZER=true NODE_ENV=production webpack --config ./config/webpack.config.js --progress",
    "start-renderer-dev": "cross-env NODE_ENV=development WHC_TARGET=electron-renderer webpack-serve --config ./config/webpack.config.js --hmr"
  },
  "build": {
    "productName": "Jarvis",
    "appId": "com.ehdfe.jarvis",
    "directories": {
      "buildResources": "resources",
      "output": "release",
      "app": "app"
    },
    "files": [
      "dist",
      "main-build",
      "child_service",
      "package.json",
      "bootstrap.js",
      "suite",
      "vendor/${os}"
    ],
    "asar": true,
    "asarUnpack": [
      "node_modules/**",
      "child_service/**"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "publish": "github"
    },
    "win": {
      "target": "nsis",
      "publish": "github"
    },
    "linux": {
      "category": "Development",
      "target": [
        "deb",
        "AppImage"
      ],
      "executableName": "Jarvis",
      "publish": "github"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EHDFE/ehdev-shell.git"
  },
  "author": "ryan.bian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EHDFE/ehdev-shell/issues"
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=5.x"
  },
  "homepage": "https://github.com/EHDFE/ehdev-shell#readme"
}
