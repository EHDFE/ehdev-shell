{
  "name": "ehdev-shell",
  "version": "0.4.0",
  "description": "An electron based developing tool for feds.",
  "keywords": [
    "ehdev",
    "jarvis",
    "develop-tool",
    "electron"
  ],
  "dependencies": {
    "@antv/g2": "^3.0.3",
    "antd": "^3.2.3",
    "classnames": "^2.2.5",
    "devtron": "^1.4.0",
    "fuzzaldrin-plus": "^0.6.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "localforage": "^1.6.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.2.1",
    "mousetrap": "^1.6.1",
    "nedb": "^1.8.0",
    "node-notifier": "^5.2.1",
    "pretty-file-icons": "^2.2.1",
    "prop-types": "^15.6.0",
    "qrcode": "^1.2.0",
    "raven": "^2.4.2",
    "raven-for-redux": "^1.2.0",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-persist": "^5.9.0",
    "redux-promise": "^0.5.3",
    "remarkable": "^1.7.1",
    "reselect": "^3.0.1",
    "semver": "^5.5.0",
    "stackblur-canvas": "^1.4.0",
    "tinycolor2": "^1.4.1",
    "xterm": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "duplicate-package-checker-webpack-plugin": "^2.1.0",
    "electron": "^1.8.2",
    "electron-builder": "~20.2.0",
    "electron-context-menu": "^0.9.1",
    "electron-log": "^2.2.14",
    "electron-react-devtools": "^0.5.3",
    "electron-updater": "^2.21.0",
    "eslint": "^4.16.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "glob": "^7.1.2",
    "html-webpack-include-assets-plugin": "^1.0.3",
    "html-webpack-plugin": "^3.0.4",
    "pre-commit": "^1.2.2",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dispatch": "^2.2.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6",
    "shebang-loader": "0.0.1",
    "source-map-support": "^0.5.3",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "~4.1.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.0.0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "scripts": {
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron .",
    "debug": "cross-env NODE_ENV=development electron --inspect=5858 .",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint --cache ./app",
    "lint-fix": "eslint --cache ./app --fix",
    "build-prod": "webpack --mode production --env.prod --config ./config/webpack.config.js --progress",
    "pack": "electron-builder --dir",
    "dist-all": "electron-builder -mw",
    "dist-mac": "electron-builder -m",
    "dist-win": "electron-builder -w",
    "postinstall": "concurrently \"electron-builder install-app-deps\"",
    "build": "concurrently \"npm run build-main \" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development webpack --mode development --env.dev --config ./config/webpack.config.dll.js --progress --verbose",
    "build-main": "cross-env NODE_ENV=production webpack --mode production --env.prod --config ./config/webpack.config.main.prod.js --progress --verbose",
    "build-renderer": "cross-env NODE_ENV=production webpack --mode production --env.prod --config ./config/webpack.config.js --progress",
    "build-renderer-analyzer": "cross-env OPEN_ANALYZER=true NODE_ENV=production webpack --mode production --env.prod --config ./config/webpack.config.js --progress",
    "start-renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --env.dev --config ./config/webpack.config.js"
  },
  "build": {
    "productName": "Jarvis",
    "appId": "com.ehdev.jarvis",
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "files": [
      "dist",
      "main-build",
      "helper",
      "child_service",
      "package.json",
      "main.js",
      "MAIN_CONFIG.js"
    ],
    "asar": true,
    "asarUnpack": [
      "node_modules/**",
      "child_service/**"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "publish": "github"
    },
    "win": {
      "target": "nsis",
      "publish": "github"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EHDFE/ehdev-shell.git"
  },
  "author": "ryan.bian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EHDFE/ehdev-shell/issues"
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=5.x"
  },
  "homepage": "https://github.com/EHDFE/ehdev-shell#readme"
}
